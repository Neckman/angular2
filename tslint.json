{
  "rules": {
    "quotemark": [true, "single", "avoid-escape"],
    "no-unused-variable": [false],
    "no-trailing-whitespace": [false],
    "member-ordering": [false],
    "no-constructor-vars": [false],
    "whitespace": [false],
    "comment-format": [false],
    "semicolon": [false],
    "align": [false], 
    "eofline": [false],
    "trailing-comma": [false],
    "no-empty": [false],
    "max-line-length": [false],
    "triple-equals": [false],
    "linebreak-style": [false],
    "no-switch-case-fall-through": [false]
  },
  "ignore": [
    "**/deps/**",
    "**/node_modules/**",
    "**/thirdparty/**",
    "**/third_party/**",
    "**/vendor/**",
    "**/**-min-**",
    "**/**-min.**",
    "**/**.min.**",
    "**/**jquery.?(ui|effects)-*.*.?(*).?(cs|j)s",
    "**/**jquery-*.*.?(*).?(cs|j)s",
    "**/prototype?(*).js",
    "**/mootools*.*.*.js",
    "**/dojo.js",
    "**/MochiKit.js",
    "**/yahoo-*.js",
    "**/yui*.js",
    "**/ckeditor*.js",
    "**/tiny_mce*.js",
    "**/tiny_mce/?(langs|plugins|themes|utils)/**",
    "**/MathJax/**",
    "**/shBrush*.js",
    "**/shCore.js",
    "**/shLegacy.js",
    "**/modernizr.custom.?(*).js",
    "**/knockout-*.*.*.debug.js",
    "**/extjs/*.js",
    "**/extjs/*.xml",
    "**/extjs/*.txt",
    "**/extjs/*.html",
    "**/extjs/*.properties",
    "**/extjs/.sencha",
    "**/extjs/docs/**",
    "**/extjs/builds/**",
    "**/extjs/cmd/**",
    "**/extjs/examples/**",
    "**/extjs/locale/**",
    "**/extjs/packages/**",
    "**/extjs/plugins/**",
    "**/extjs/resources/**",
    "**/extjs/src/**",
    "**/extjs/welcome/**",
    "bower_components/**",
    "src/server/data.ts"
  ]
}
